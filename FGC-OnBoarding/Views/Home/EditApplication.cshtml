                                    </tr>
                                    @{
                                        var MailTo = "mailto:" + rep.Email;
                                    }
                                    <tr>
                                        <td style="font-weight:bold;  width:250px;">Email</td>
                                        <td>
                                            <a href="@Mail"><span class="AuthemailOld"> @rep.Email</span></a>
                                            <a style="display:none" id="Authemail" class="ml-4 AuthorizeEditIcon" data-toggle="modal" data-target="#modal-Buisness-Auth"><i class="fas fa-pencil-alt text-info " title="Edit"></i></a>
                                        </td>
                                    </tr>
                                    @if (rep.BuisnessProfileId == 1)
                                    {
                                        <tr>
                                            <td style="font-weight:bold;  width:250px;">Position in Buisness</td>
                                            <td>
                                                <span class="AuthpositioninbuisnessOld"> @rep.PositionInBuisness</span>
                                                <a style="display:none" id="Authpositioninbuisness" class="ml-4 AuthorizeEditIcon" data-toggle="modal" data-target="#modal-Buisness-Auth"><i class="fas fa-pencil-alt text-info " title="Edit"></i></a>
                                            </td>
                                        </tr>
                                    }
                                    else if (rep.BuisnessProfileId == 2)
                                    {
                                        <tr>
                                            <td style="font-weight:bold;  width:250px;">Role in Charity</td>
                                            <td>
                                                <span class="AuthroleincharityOld"> @rep.RoleIncharity</span>
                                                <a style="display:none" id="Authroleincharity" class="ml-4 AuthorizeEditIcon" data-toggle="modal" data-target="#modal-Buisness-Auth"><i class="fas fa-pencil-alt text-info " title="Edit"></i></a>
                                            </td>
                                        </tr>
                                    }
                                    else if (rep.BuisnessProfileId == 3 || rep.BuisnessProfileId == 4)
                                    {
                                        <tr>
                                            <td style="font-weight:bold;  width:250px;">Position in Company</td>
                                            <td>
                                                <span class="AuthpositionincompanyOld"> @rep.PositionInComany</span>
                                                <a style="display:none" id="Authpositionincompany" class="ml-4 AuthorizeEditIcon" data-toggle="modal" data-target="#modal-Buisness-Auth"><i class="fas fa-pencil-alt text-info " title="Edit"></i></a>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <hr />
                    i = i + 1;
                }
            </div>
        </div>
        @*///////////////////////////////////////////////////////////////////////////////Authorized Representative End*@
    </div>
</div>
