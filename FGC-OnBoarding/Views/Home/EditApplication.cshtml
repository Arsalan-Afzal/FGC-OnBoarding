@model FGC_OnBoarding.Models.ParentProfile
@{
    Layout = "CustomerLayout";
}
@*//////////////////////////////////////////////Buisness Profile Main Start //////////////////*@
<div id="link1" class="card card-margin">
    <div style="background-color:white !important" class="card-header mycardheader">
        <h3 class="card-title">Buisness Profile</h3>
        <div class="card-tools">
        </div>
    </div>
    <div class="card-body">
        @*///////////////////////////////////////////////////////////////////////////////Buisness Profile Start*@
        <div class="card card-margin">
            <div style="background-color: #f5f5f5" class="card-header mycardheader">
                <h3 class="card-title">Service Requirment</h3>
                <div class="card-tools">
                    <button type="button" data-toggle="modal" data-target="#modal-Currency" class=" btn btn-success  mb-2 pull-right">

                        <i class="fa fa-paste text-white" title="Edit"></i>Edit
                    </button>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
                        <h4>In which currency do you wish your FGC accounts to be maintained?</h4>
                        @{
                            List<string> currencyList = new List<string>();

                            if (((string)ViewBag.currency).Contains(','))
                            {

                                var currencyarray = ((string)ViewBag.currency).Split(',');
                                foreach (var Curr in currencyarray)
                                {
                                    if (Curr != "")
                                    {
                                        <strong>- @Curr<br></strong>
                                    }
                                }
                            }
                            else
                            {
                                <strong>- @ViewBag.currency<br></strong>
                            }
                        }

                    </div>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
                        <h4>Type of Business</h4>
                        <strong style="text-transform: capitalize;">- @ViewBag.BuisnessTypeName</strong><br>
                    </div>
                </div>
            </div>
            @if (ViewBag.BuisnesssTypeId != 2)
            {
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-12">
                            <h4>Business Sector</h4>
                            <strong>- @Model.objBuisness.BuisnessSector.Name</strong><br>
                        </div>
                    </div>
                </div>
            }
        </div>
        @*///////////////////////////////////////////////////////////////////////////////Buisness Profile END*@
        <div class="card card-margin">
            <div style="background-color: #f5f5f5" class="card-header mycardheader">
                <h3 class="card-title">@*<i class="nav-icon fas fa-tachometer-alt"></i>*@BUISNESS PROFILE</h3>
                <div class="card-tools">
                    <button onclick="BuisnessProfileEdit()" type="button" class=" btn btn-success  mb-2 pull-right">

                        <i class="fa fa-paste text-white" title="Edit"></i>Edit
                    </button>
                    @*else
                        {*@
                    <button style="display:none" onclick="CancelBuisnessProfileEdit()" type="button" class=" btn btn-secondary mr-2  mb-2 pull-right buisnessprofilecancel">
                        <!--!--><i class="fa fa-times text-white " title="cancel"></i>Cancel
                    </button>
                    @*}*@
                </div>
            </div>
            <div class="card-body">
                <table class="table table-bordered">
                    <tbody>
                        <tr>
                            <td style="font-weight:bold;  width:250px;">Business Name</td>
                            <td>
                                <span class="BuisnessnameOld"> @Model.objBuisness.BuisnessName</span>
                                <a style="display:none" id="Buisnessname" class="ml-4 BuisnessEditIcon" data-toggle="modal" data-target="#modal-Buisness-Profile"><i class="fas fa-pencil-alt text-info " title="Edit"></i></a>

                            </td>
